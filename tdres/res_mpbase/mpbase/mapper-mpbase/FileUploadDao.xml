<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.tangdi.production.mpbase.dao.FileUploadDao">	<!--配置一个resultMap 指定返回的类型 -->      <resultMap id="FileMap" type="FileInf">          <result property="id"      		column="ID" 		/>        <result property="moduleName"   column="MODULENAME" />        <result property="tableName"    column="TABLENAME" 	/>        <result property="pkId" 		column="PKID" 		/>        <result property="lx" 			column="LX" 		/>        <result property="orderNum" 	column="ORDERNUM" 	/>        <result property="fjName" 		column="FJNAME" 	/>        <result property="fjPath" 		column="FJPATH" 	/>        <result property="fjo" 			column="FJO" 		/>        <result property="fjt" 			column="FJT" 		/>        <result property="sfsx" 		column="SFSX" 		/>        <result property="dx" 			column="DX"   		/>    </resultMap>		<select id="selectEntity" resultMap="FileMap" parameterType="FileInf"> 		SELECT  ID,		 		MODULENAME,		 		TABLENAME,		 		PKID,		 		LX,		 		ORDERNUM,		 		FJNAME,		 		FJPATH,		 		FJO,		 		FJT,		 		SFSX,		 		DX  		FROM  				ATTACHMENT   		WHERE				ID = #{id} 			</select>		<insert id="insertEntity" parameterType="FileInf">	  	INSERT INTO ATTACHMENT 	  		(ID,TABLENAME,PKID,LX,ORDERNUM,FJNAME,FJPATH,FJO,FJT,SFSX,DX,DPI)	  	VALUES 	  		(#{id,jdbcType=VARCHAR},#{tableName,jdbcType=VARCHAR},#{pkId,jdbcType=VARCHAR},#{lx,jdbcType=VARCHAR},#{orderNum,jdbcType=VARCHAR},#{fjName,jdbcType=VARCHAR},#{fjPath,jdbcType=VARCHAR},#{fjo,jdbcType=VARCHAR},#{fjt,jdbcType=VARCHAR},#{sfsx,jdbcType=VARCHAR},#{dx,jdbcType=VARCHAR},#{dpi,jdbcType=VARCHAR})	</insert>		<update id="updateEntity" parameterType="FileInf"> 		UPDATE  				ATTACHMENT  		SET  				SFSX = '1'  		WHERE  				ID = #{id}	</update></mapper>