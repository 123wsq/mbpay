<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.tangdi.production.tdbase.dao.AttachmentDao">	<!--配置一个resultMap 指定返回的类型 -->      <resultMap id="Attachment" type="Attachment">          <result column="id"      	property="id" 			/>        <result column="moduleName" property="moduleName" 	/>        <result column="tableName"  property="tableName" 	/>        <result column="pkId" 		property="pkId" 		/>        <result column="lx" 		property="lx" 			/>        <result column="orderNum" 	property="orderNum" 	/>        <result column="fjName" 	property="fjName" 		/>        <result column="fjPath" 	property="fjPath" 		/>        <result column="fjo" 		property="fjo" 			/>        <result column="fjt" 		property="fjt" 			/>        <result column="sfsx" 		property="sfsx" 		/>        <result column="dx" 		property="dx"   		/>    </resultMap>		<select id="selectList" resultType="Attachment" parameterType="java.util.Map"> 		SELECT * FROM Attachment   		<where>	 		<if test="ID != null">				ID = #{ID} 			</if>			<if test="TABLENAME != null">				AND TABLENAME = #{TABLENAME}  			</if>			<if test="PKID != null">				AND PKID = #{PKID} 			</if>		</where>	</select>		<select id="selectEntity" resultType="Attachment" parameterType="Attachment"> 		SELECT * FROM ATTACHMENT   		<where>	 		<if test="id != null">				ID = #{id} 			</if>			<if test="tableName != null">				AND TABLENAME = #{tableName}  			</if>			<if test="pkId != null">				AND PKID = #{pkId} 			</if>			<if test="lx != null">				AND LX = #{lx} 			</if>			<if test="orderNum != null">				AND ORDERNUM = #{orderNum} 			</if>		</where>	</select>		<insert id="insertEntity" parameterType="Attachment">	  	INSERT INTO Attachment (ID,TABLENAME,PKID,LX,ORDERNUM,FJNAME,FJPATH,FJT,SFSX,DX,DPI)	  	VALUES (#{id,jdbcType=VARCHAR},#{tableName,jdbcType=VARCHAR},#{pkId,jdbcType=VARCHAR},#{lx,jdbcType=VARCHAR},#{orderNum,jdbcType=VARCHAR},#{fjName,jdbcType=VARCHAR},#{fjPath,jdbcType=VARCHAR},#{fjt,jdbcType=VARCHAR},#{sfsx,jdbcType=VARCHAR},#{dx,jdbcType=VARCHAR},#{dpi,jdbcType=VARCHAR})	</insert>		<update id="updateNewEntity" parameterType="java.util.Map"> 		UPDATE Attachment  		SET PKID = #{PKID}, 		SFSX = '1'  		WHERE ID in 		<foreach item="item" index="index" collection="IDS" open="(" separator="," close=")">	        #{item}	  	</foreach>	</update>		<update id="updateHasSaveEntity" parameterType="Attachment"> 		UPDATE Attachment  		<set>	      	<if test="fjName != null">fjName = #{fjName},</if>	      	<if test="fjPath != null">fjPath = #{fjPath},</if>	      	<if test="fjt != null">fjt = #{fjt}</if>	    </set> 		WHERE ID = #{ID}	</update>		<delete id="deleteEntity" parameterType="Attachment"> 		DELETE FROM Attachment WHERE ID = #{id}	</delete>		<delete id="deleteSameTypeEntity" parameterType="Attachment"> 		DELETE FROM Attachment  		WHERE TABLENAME = #{tableName}  		AND PKID = #{pkId}  		AND LX = #{lx}  		AND ORDERNUM = #{orderNum}  		AND ID != #{id}	</delete>		<delete id="deleteLinkEntity" parameterType="Attachment"> 		DELETE FROM Attachment WHERE PKID = #{pkId} AND TABLENAME = #{tableName} 	</delete></mapper>